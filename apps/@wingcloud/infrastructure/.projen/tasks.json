{
  "tasks": {
    "build": {
      "name": "build",
      "description": "Full release build",
      "steps": [
        {
          "spawn": "pre-compile"
        },
        {
          "spawn": "compile"
        },
        {
          "spawn": "post-compile"
        },
        {
          "spawn": "test"
        },
        {
          "spawn": "package"
        }
      ]
    },
    "compile": {
      "name": "compile",
      "description": "Only compile"
    },
    "default": {
      "name": "default",
      "description": "Synthesize project files",
      "steps": [
        {
          "exec": "npx projen default",
          "cwd": "../../.."
        }
      ]
    },
    "deploy": {
      "name": "deploy",
      "steps": [
        {
          "exec": "node ./bin/terraform.mjs -chdir=target/main.tfaws apply -input=false -auto-approve tfplan"
        }
      ]
    },
    "dev": {
      "name": "dev",
      "description": "Start the development task",
      "steps": [
        {
          "exec": "node ./bin/wing.mjs it main.w"
        }
      ]
    },
    "install": {
      "name": "install",
      "description": "Install project dependencies and update lockfile (non-frozen)",
      "steps": [
        {
          "exec": "pnpm install --no-frozen-lockfile"
        }
      ]
    },
    "install:ci": {
      "name": "install:ci",
      "description": "Install project dependencies using frozen lockfile",
      "steps": [
        {
          "exec": "pnpm install --frozen-lockfile"
        }
      ]
    },
    "lint": {
      "name": "lint",
      "description": "Lint the code",
      "steps": [
        {
          "exec": "eslint --fix --ext .ts,.tsx ."
        }
      ]
    },
    "package": {
      "name": "package",
      "description": "Creates the distribution package"
    },
    "plan": {
      "name": "plan",
      "steps": [
        {
          "exec": "node ./bin/terraform.mjs -chdir=target/main.tfaws plan -input=false -out=tfplan"
        }
      ]
    },
    "post-compile": {
      "name": "post-compile",
      "description": "Runs after successful compilation"
    },
    "postinstall": {
      "name": "postinstall",
      "description": "Run after install"
    },
    "pre-compile": {
      "name": "pre-compile",
      "description": "Prepare the project for compilation"
    },
    "terraformInit": {
      "name": "terraformInit",
      "steps": [
        {
          "exec": "node ./bin/terraform.mjs -chdir=target/main.tfaws init -input=false"
        }
      ]
    },
    "test": {
      "name": "test",
      "description": "Run tests",
      "steps": [
        {
          "exec": "node ./bin/wing.mjs test main.w"
        }
      ]
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
